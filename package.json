{
  "name": "graphql-sequelize-postgres",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "apollo-server-express": "^1.1.7",
    "apollo-tracing": "^0.0.9",
    "body-parser": "^1.18.1",
    "bunyan": "^1.8.12",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-bunyan-logger": "^1.3.2",
    "graphql": "^0.11.7",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prettier-standard": "^7.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^3.0.0-3",
    "sequelize-cockroachdb": "^1.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "test": "yarn lint",
    "lint": "prettier-standard --fix src/**/*.js",
    "start": "DEBUG=1 NODE_PATH=./src node src",
    "start:dev": "NODE_PATH=./src nodemon --exec \"yarn lint && node\" src | bunyan"
