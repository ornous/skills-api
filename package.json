  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier-standard",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  },
  "keywords": [
    "graphql",
    "api",
    "skills"
